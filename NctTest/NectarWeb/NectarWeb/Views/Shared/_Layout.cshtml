<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - Nectar</title>
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
  <link rel="stylesheet" href="~/css/site.css?id=4" />
</head>
<body>
  <header>
    <nav style="background-color: #fe9800!important;" class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
      <div class="container">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Nectar</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse navbar-items">
          <ul class="navbar-nav flex-grow-1">
            <li class="nav-item">
              <a id="askBeekeeper" class="nav-link text-dark" onclick="window.location.href='/Contacter/Apiculteur'">Particulier</a>
            </li>
            <li class="nav-item">
              <a id="beekeeperPage" class="nav-link text-dark" onclick="window.location.href='/Apiculteur/'">Apiculteur</a>
            </li>
            <li class="nav-item">
              <a id="blog" class="nav-link text-dark" asp-area="" asp-controller="Apiculteur" asp-action="Privacy">Blog</a>
            </li>
            <li class="nav-item">
              <a id="contactUs" class="nav-link text-dark" asp-area="" asp-controller="Apiculteur" asp-action="Privacy">Contact</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </header>
  <div>
      @RenderBody()
  </div>

  <footer class="border-top footer text-muted">
    <div class="container">
      &copy; 2020 - NectarWeb - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
  </footer>
  <script src="~/lib/jquery/dist/jquery.min.js"></script>
  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="~/js/site.js" asp-append-version="true"></script>
  @RenderSection("Scripts", required: false)
</body>
</html>
